    Attribute VB_GlobalNameSpace = False
    Attribute VB_Creatable = True
    Attribute VB_PredeclaredId = True
    Attribute VB_Exposed = True
    Option Explicit

    /* 
    This enumeration implements error messages
    To allow consistem namin of error message we introduce the alOrd enum
    snd a dictionary that maps erral to the actual error number
    */

    Private Const MY_TYPENAME              As String = "enumAlert"

    Public Enum AlertOrd ' using the prefix al
        /*
        The first part of this enumeration reflects alError names
        However we are not using Alerts as error messages
