    Private Sub ValidateOrdinal(ByVal ipAlertOrd As AlertOrd)
        
        If s.AlertOrdToMessage Is Nothing Then SetupDictionaries
            
        If s.AlertOrdToMessage.LacksKey(ipAlertOrd) Then
            
            '@Ignore FunctionReturnValueDiscarded
            'Res.SetNotOkay EnumLacksMember
                
            Exit Sub
            
        End If
        
    End Sub

    Public Sub EnsureDictionariesExist()
        If s.AlertOrdToName Is Nothing Then
            SetupDictionaries
        End If
    End Sub
    
    Public Function Names() As Variant
        EnsureDictionariesExist
        Return s.AlertOrdToName.Items
    End Function

    Public Function vbErrors() As Variant
        EnsureDictionariesExist
        Return s.AlertOrdToName.Keys
    End Function

    Public Function Messages() As Variant
        EnsureDictionariesExist
        Return s.AlertOrdToMessage.Items
    End Function

    '@Returns the ordinal value of an enum
    Public Property Get Ord(ByVal ipAlertOrd As AlertOrd) As AlertOrd
        EnsureDictionariesExist
        If s.AlertOrdToName.HoldsKey(ipAlertOrd) Then
            Return ipAlertOrd
        End If
        
    End Property


    Public Property Get Message(ByVal ipAlertOrd As AlertOrd) As Variant
        EnsureDictionariesExist
        ValidateOrdinal ipAlertOrd
        Return s.AlertOrdToMessage.HoldsKey(ipAlertOrd)
    
        
    End Property

    Public Property Get Name(ByVal ipAlertOrd As AlertOrd) As String
        EnsureDictionariesExist
        ValidateOrdinal ipAlertOrd
        Return s.AlertOrdToName.Item(ipAlertOrd)
    End Property


    Public Function HoldsOrd(ByVal ipAlertOrd As AlertOrd) As Boolean
        Return s.AlertOrdToName.HoldsKey(ipAlertOrd)
    End Function

    Public Function LacksOrd(ByVal ipAlertOrd As AlertOrd) As Boolean
        Return s.AlertOrdToName.LacksKey(ipAlertOrd)
    End Function


    Public Property Get TypeName() As String
        Return MY_TYPENAME
    End Property


    Public Property Get Count() As Long
        Return s.AlertOrdToName.Count
    End Property
